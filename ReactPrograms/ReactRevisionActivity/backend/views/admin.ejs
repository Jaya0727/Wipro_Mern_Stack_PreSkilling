<!DOCTYPE html>
<html>
<body>

<h2>Product List</h2>

<ul>
  <% products.forEach(product => { %>
    <li>
      <strong>Name:</strong> <%= product.name %> <br>
      <strong>Price:</strong> <%= product.price %> <br>
      <strong>Category:</strong> <%= product.category %>
    </li>
    <hr>
  <% }) %>
</ul>

</body>
</html>